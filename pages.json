{
	"easycom": {
		"autoscan": true,
		"custom": {
			// uni-ui 规则如下配置
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [
		//pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/tabbar/home/home",
			"style": {
				"navigationBarTitleText": "今天吃什么？"
			}
		},
		{
			"path": "pages/tabbar/recommend/recommend",
			"style": {}
		},
		{
			"path": "pages/tabbar/personInfo-change/personInfo-change",
			"style": {}
		},
		{
			"path": "pages/tabbar/food-detail/food-detail",
			"style": {}
		},
		{
			"path": "pages/tabbar/my/my",
			"style": {}
		},
		{
			"path": "pages/tabbar/random/random",
			"style": {}
		},
		{
			"path": "pages/tabbar/chat/chat",
			"style": {}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "今天吃什么",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"borderStyle": "black",
		"backgroundColor": "#333",
		"color": "#8F8F94",
		"selectedColor": "#f33e54",
		"list": [{
				"pagePath": "pages/tabbar/home/home",
				"iconPath": "static/img/tabbar/home.png",
				"selectedIconPath": "static/img/tabbar/homeactive.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/tabbar/recommend/recommend",
				"iconPath": "static/img/tabbar/guanzhu.png",
				"selectedIconPath": "static/img/tabbar/guanzhuactive.png",
				"text": "推荐"
			},
			{
				"pagePath": "pages/tabbar/random/random",
				"iconPath": "static/img/tabbar/random.png",
				"selectedIconPath": "static/img/tabbar/randomactive.png",
				"text": "随机"
			},
			{
				"pagePath": "pages/tabbar/chat/chat",
				"iconPath": "static/img/tabbar/chat.png",
				"selectedIconPath": "static/img/tabbar/chatactive.png",
				"text": "chatgpt"
			},
			{
				"pagePath": "pages/tabbar/my/my",
				"iconPath": "static/img/tabbar/me.png",
				"selectedIconPath": "static/img/tabbar/meactive.png",
				"text": "我"
			}
		]
	}
}
